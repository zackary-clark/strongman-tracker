{
    "name": "strongman-tracker",
    "version": "0.0.3",
    "license": "MIT",
    "scripts": {
        "analyze": "webpack --json > stats.json",
        "build": "yarn install && webpack",
        "dev": "webpack-dashboard -- webpack-dev-server",
        "docker:buildlocal": "yarn build && docker rmi -f zackaryclark/strongman-tracker && docker build ./ -f Local.Dockerfile -t zackaryclark/strongman-tracker",
        "docker:buildrelease": "yarn build && docker build ./ -f Dockerfile -t zackaryclark/strongman-tracker:latest -t zackaryclark/strongman-tracker:$npm_package_version",
        "docker:pushrelease": "docker push zackaryclark/strongman-tracker:$npm_package_version && docker push zackaryclark/strongman-tracker:latest",
        "minikube:create": "kubectl create -f kubernetes/local-deployment.yml && kubectl apply -f kubernetes/local-nodeport.yml",
        "minikube:start": "yarn docker:buildlocal && yarn minikube:create && minikube service strongman-tracker --url",
        "minikube:stop": "kubectl delete deployment strongman-tracker --ignore-not-found && kubectl delete services strongman-tracker --ignore-not-found",
        "test": "jest",
        "preversion": "yarn test",
        "postversion": "git push --tags && git push && yarn docker:buildrelease && yarn docker:pushrelease && echo -e \"\n\\033[0;32m  <==  Released Client v$npm_package_version  ==>  \\033[0m\n\""
    },
    "dependencies": {
        "@babel/runtime": "7.15.4",
        "@material-ui/core": "4.12.3",
        "@material-ui/icons": "4.11.2",
        "@material-ui/lab": "4.0.0-alpha.60",
        "axios": "0.21.4",
        "material-table": "1.57.2",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-router-dom": "5.3.0"
    },
    "devDependencies": {
        "@babel/core": "7.15.5",
        "@babel/plugin-proposal-class-properties": "7.14.5",
        "@babel/plugin-transform-runtime": "7.15.0",
        "@babel/preset-env": "7.15.6",
        "@babel/preset-react": "7.14.5",
        "@babel/preset-typescript": "7.15.0",
        "@testing-library/dom": "8.5.0",
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "12.1.0",
        "@testing-library/user-event": "13.2.1",
        "@types/jest": "27.0.1",
        "@types/material-ui": "0.21.9",
        "@types/react": "16.14.15",
        "@types/react-dom": "16.9.14",
        "@types/react-router-dom": "5.1.9",
        "@typescript-eslint/eslint-plugin": "4.31.1",
        "@typescript-eslint/parser": "4.31.1",
        "babel-jest": "27.2.0",
        "babel-loader": "8.2.2",
        "babel-plugin-syntax-dynamic-import": "6.18.0",
        "eslint": "7.32.0",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-jest-dom": "3.9.2",
        "eslint-plugin-react": "7.25.1",
        "eslint-plugin-testing-library": "4.12.2",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.3.2",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.2.0",
        "ts-loader": "9.2.5",
        "typescript": "4.4.3",
        "webpack": "5.53.0",
        "webpack-cli": "4.8.0",
        "webpack-dashboard": "3.3.6",
        "webpack-dev-server": "4.2.1"
    },
    "private": true
}
