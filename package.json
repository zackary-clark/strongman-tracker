{
    "name": "strongman-tracker",
    "version": "0.2.4",
    "license": "GPL-3.0-only",
    "scripts": {
        "analyze": "webpack --json > stats.json",
        "prebuild": "npm install && npm run generate",
        "build": "webpack",
        "predev": "npm install",
        "dev": "concurrently \"webpack serve --mode development --env development\" \"graphql-codegen --watch\"",
        "docker:buildlocal": "npm run build && docker rmi -f zackaryclark/strongman-tracker && docker build ./ -t zackaryclark/strongman-tracker",
        "docker:buildrelease": "npm run build && docker build ./ -t zackaryclark/strongman-tracker:latest -t zackaryclark/strongman-tracker:$npm_package_version",
        "docker:check": "./scripts/checkForDockerDaemon.sh",
        "docker:pushrelease": "docker push zackaryclark/strongman-tracker:$npm_package_version && docker push zackaryclark/strongman-tracker:latest",
        "docker:release": "npm run docker:buildrelease && npm run docker:pushrelease",
        "generate": "graphql-codegen",
        "git:pushtags": "git push --tags && git push",
        "kubelocal:use": "kubectl config use-context docker-desktop",
        "kubelocal:create": "kubectl create -f kubernetes/local-deployment.yml && kubectl apply -f kubernetes/local-nodeport.yml",
        "kubelocal:start": "npm run docker:check && npm run kubelocal:use && npm run docker:buildlocal && npm run kubelocal:create",
        "kubelocal:stop": "npm run kubelocal:use && kubectl delete deployment strongman-tracker --ignore-not-found && kubectl delete services strongman-tracker --ignore-not-found",
        "kubeprod:release": "kubectl config use-context prod && kubectl rollout restart deployment strongman-tracker-deployment",
        "print:release": "echo \"\n\\033[0;32m  <==  Released Client v$npm_package_version  ==>  \\033[0m\n\"",
        "test": "jest",
        "preversion": "npm test && npm run docker:check",
        "postversion": "npm run git:pushtags && npm run docker:release && sleep 5 && npm run kubeprod:release && npm run print:release"
    },
    "dependencies": {
        "@apollo/client": "3.6.9",
        "@babel/runtime": "7.18.9",
        "@emotion/react": "11.9.3",
        "@emotion/styled": "11.9.3",
        "@mui/icons-material": "5.8.4",
        "@mui/material": "5.9.1",
        "@mui/x-date-pickers": "^5.0.0-beta.2",
        "date-fns": "2.29.1",
        "graphql": "16.5.0",
        "keycloak-js": "18.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.3.0"
    },
    "devDependencies": {
        "@babel/core": "7.18.9",
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/plugin-transform-runtime": "7.18.9",
        "@babel/preset-env": "7.18.9",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@graphql-codegen/cli": "2.9.1",
        "@graphql-codegen/typescript": "2.7.2",
        "@graphql-codegen/typescript-operations": "2.5.2",
        "@graphql-codegen/typescript-react-apollo": "3.3.2",
        "@testing-library/dom": "8.16.0",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.3.0",
        "@testing-library/user-event": "14.4.2",
        "@types/css-mediaquery": "0.1.1",
        "@types/jest": "28.1.6",
        "@types/react": "18.0.15",
        "@types/react-dom": "18.0.6",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "5.30.7",
        "@typescript-eslint/parser": "5.30.7",
        "babel-jest": "28.1.3",
        "babel-loader": "8.2.5",
        "babel-plugin-syntax-dynamic-import": "6.18.0",
        "concurrently": "7.0.0",
        "css-mediaquery": "0.1.2",
        "eslint": "8.20.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jest-dom": "4.0.2",
        "eslint-plugin-react": "7.30.1",
        "eslint-plugin-testing-library": "5.5.1",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.5.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "28.1.3",
        "jest-environment-jsdom": "28.1.3",
        "ts-loader": "9.3.1",
        "typescript": "4.7.4",
        "webpack": "5.73.0",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.9.3"
    },
    "private": true
}
